<?xml version="1.0"
      encoding="UTF-8"?>

<production-profile xmlns="http://schemas.nuix.com/2016/production-profile" version="1"
                    created-by="8.2.0.123">
  <id>d0107862-1c9d-4652-b6a3-d20513f2bc30</id>
  <headers-and-footers>
    <header-left>
      <stamper-type>NAME</stamper-type>
      <font-enabled>false</font-enabled>
    </header-left>
    <header-centre>
      <stamper-type>NOTHING</stamper-type>
    </header-centre>
    <header-right>
      <stamper-type>GUID</stamper-type>
      <font-enabled>false</font-enabled>
    </header-right>
    <footer-left>
      <stamper-type>DOCUMENT_NUM</stamper-type>
      <font-enabled>false</font-enabled>
    </footer-left>
    <footer-centre>
      <stamper-type>PAGE_NUM</stamper-type>
      <font-enabled>false</font-enabled>
    </footer-centre>
    <footer-right>
      <stamper-type>PAGE_COUNT</stamper-type>
      <font-enabled>false</font-enabled>
    </footer-right>
    <show-header-line>true</show-header-line>
    <show-footer-line>true</show-footer-line>
    <increase-page-size>true</increase-page-size>
  </headers-and-footers>
  <text-export-settings>
    <wrap-lines-at>false</wrap-lines-at>
    <columns>80</columns>
    <charset>UTF-8</charset>
    <line-separators>\r\n</line-separators>
  </text-export-settings>
  <export-rules>
    <export-rule>
      <item-condition>
        <conjunction>ANY</conjunction>
      </item-condition>
      <native-enabled>true</native-enabled>
      <slipsheet-replace-native-enabled>false</slipsheet-replace-native-enabled>
      <tiff-enabled>false</tiff-enabled>
    </export-rule>
  </export-rules>
  <use-multipage-tiff>false</use-multipage-tiff>
  <numbering-enabled>true</numbering-enabled>
  <numbering-settings>
    <numbering-type>standard</numbering-type>
    <numbering-strategy>
      <type>DOCUMENT_ID</type>
      <prefix>DOC-</prefix>
      <padding>9</padding>
      <number>1</number>
      <group-document-pages>true</group-document-pages>
      <group-family-documents>false</group-family-documents>
    </numbering-strategy>
  </numbering-settings>
  <export-output-enabled>true</export-output-enabled>
  <export-type>CONCORDANCE</export-type>
  <file-naming-strategies>
    <file-naming-strategy>
      <file-type>NATIVE</file-type>
      <naming-type>DocumentId</naming-type>
      <sub-folder>NATIVE</sub-folder>
      <file-name-suffix />
    </file-naming-strategy>
    <file-naming-strategy>
      <file-type>TEXT</file-type>
      <naming-type>DocumentId</naming-type>
      <sub-folder>TEXT</sub-folder>
      <file-name-suffix />
    </file-naming-strategy>
    <file-naming-strategy>
      <file-type>PDF</file-type>
      <naming-type>DocumentId</naming-type>
      <sub-folder>IMAGE</sub-folder>
      <file-name-suffix />
    </file-naming-strategy>
  </file-naming-strategies>
  <load-file-settings>
    <charset>windows-1252</charset>
    <line-separator>\r\n</line-separator>
    <path-separator>\</path-separator>
    <break-files>false</break-files>
    <metadata-profile>Context</metadata-profile>
  </load-file-settings>
</production-profile>